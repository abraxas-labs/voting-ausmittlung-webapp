{
  "name": "@abraxas/voting-ausmittlung",
  "version": "0.0.0-development",
  "scripts": {
    "prepare": "husky install",
    "start": "(shx rm -rf dist/ausmittlung-lib || true) && concurrently -k npm:start:lib npm:start:erfassung npm:start:monitoring",
    "start:sta": "shx rm -rf dist/ausmittlung-lib && concurrently -k npm:start:lib npm:start:erfassung:sta npm:start:monitoring:sta",
    "start:lib": "ng build --project=ausmittlung-lib --watch",
    "start:erfassung": "wait-on dist/ausmittlung-lib && ng serve --project=erfassung --port 4300",
    "start:monitoring": "wait-on dist/ausmittlung-lib && ng serve --project=monitoring --project=monitoring --port 4400",
    "start:erfassung:sta": "wait-on dist/ausmittlung-lib && ng serve --project=erfassung -c=local-sta --port 4300",
    "start:monitoring:sta": "wait-on dist/ausmittlung-lib && ng serve --project=monitoring -c=local-sta --port 4400",
    "build:lib": "ng build --project=ausmittlung-lib",
    "build:erfassung": "npm run build:lib && cd ./projects/erfassung && ng build --project=erfassung --configuration production",
    "build:monitoring": "npm run build:lib && cd ./projects/monitoring && ng build --project=monitoring --configuration production",
    "test:integration:erfassung:debug": "testcafe chrome ./e2e/stories/erfassung/**/*.ts --hostname=localhost --port=4300 --debug-on-fail",
    "test:integration:monitoring:debug": "testcafe chrome ./e2e/stories/monitoring/**/*.ts --hostname=localhost --port=4400 --debug-on-fail",
    "test:integration:erfassung:ci": "concurrently -s first -k npm:start:erfassung \"wait-on http://localhost:4300 && testcafe chrome:headless ./e2e/stories/erfassung/**/*.ts --hostname=localhost --port=4300 --quarantine-mode\"",
    "test:integration:monitoring:ci": "concurrently -s first -k npm:start:monitoring \"wait-on http://localhost:4400 && testcafe chrome:headless ./e2e/stories/monitoring/**/*.ts --hostname=localhost --port=4400 --quarantine-mode\"",
    "lint": "prettier --check ./projects/**/*.{ts,scss} && ng lint && stylelint \"./projects/**/*.scss\"",
    "lint:ng": "ng lint",
    "fix": "prettier --write ./projects/**/*.{ts,scss} && ng lint --fix && stylelint \"./projects/**/*.scss\" --fix"
  },
  "private": true,
  "dependencies": {
    "@abraxas/base-components": "14.18.2",
    "@abraxas/voting-ausmittlung-service-proto": "1.99.2",
    "@abraxas/voting-lib": "2.6.0",
    "@angular/animations": "13.3.11",
    "@angular/cdk": "13.3.9",
    "@angular/cdk-experimental": "^13.3.9",
    "@angular/common": "13.3.11",
    "@angular/compiler": "13.3.11",
    "@angular/core": "13.3.11",
    "@angular/forms": "13.3.11",
    "@angular/material": "13.3.9",
    "@angular/material-moment-adapter": "13.3.9",
    "@angular/platform-browser": "13.3.11",
    "@angular/platform-browser-dynamic": "13.3.11",
    "@angular/router": "13.3.11",
    "@ngx-translate/core": "13.0.0",
    "deepmerge": "4.2.2",
    "google-protobuf": "3.21.0",
    "grpc-web": "1.3.1",
    "moment": "2.29.4",
    "ngx-currency": "2.5.3",
    "ngx-drag-drop": "13.0.1",
    "ngx-mask": "13.1.15",
    "ngx-moment": "6.0.2",
    "otplib": "12.0.1",
    "rxjs": "7.5.6",
    "tslib": "2.4.0",
    "zone.js": "0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.9",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "13.3.9",
    "@angular/compiler-cli": "13.3.11",
    "@angular/language-service": "13.3.11",
    "@angularclass/hmr": "3.0.0",
    "@types/google-protobuf": "3.15.6",
    "@types/jest": "28.1.6",
    "@types/jsdom": "20.0.0",
    "@types/node": "18.6.5",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "concurrently": "7.3.0",
    "eslint": "8.21.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-preset-angular": "12.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "lint-staged": "13.0.3",
    "ng-lint-staged": "12.0.4",
    "ng-packagr": "13.3.1",
    "prettier": "2.7.1",
    "prettier-stylelint": "0.4.2",
    "shx": "0.3.4",
    "stylelint": "14.9.1",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-scss": "4.3.0",
    "testcafe": "1.20.1",
    "typescript": "4.6.4",
    "wait-on": "6.0.1"
  }
}
